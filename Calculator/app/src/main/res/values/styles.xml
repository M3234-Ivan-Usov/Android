<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="DarkBack" parent="Theme.AppCompat">
        <item name="android:background">#1C1C1C</item>
        <item name="android:textColor">#E8E8E8</item>
    </style>
    <style name="DarkDigit" parent="Theme.AppCompat">
        <item name="android:textColor">#FF7F00</item>
        <item name="android:background">#1C1C1C</item>
    </style>
    <style name="DarkOp" parent="Theme.AppCompat">
        <item name="android:textColor">#FFA500</item>
        <item name="android:background">#1C1C1C</item>
    </style>
    <style name="DarkOther" parent="Theme.AppCompat">
        <item name="android:textColor">#EE9A49</item>
        <item name="android:background">#1C1C1C</item>
    </style>

</resources>